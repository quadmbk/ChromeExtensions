{
    "manifest_version": 3,
    "name": "YouTube Feed Reset",
    "version": "1.0",
    "description": "Customize YouTube home feed based on keywords.",
    "permissions": ["storage", "scripting", "activeTab"],
    "host_permissions": ["https://www.youtube.com/*"],
    "action": {
      "default_popup": "popup.html"     
    },

    "content_scripts": [{
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
    }]

   
  }
  